		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="roomID" type="long" />
		<column name="customerID" type="String" />
		<column name="bookId" type="long" />
		<column name="rent" type="float" />
		<column name="checkoutDate" type="Date" />
		<column name="duration" type="long" />
		<column name="checkinDate" type="Date" />
		<column name="cancelledDate" type="Date" />

		<!-- Order -->

		<order by="asc">
			<order-column name="createDate" />
		</order>
	</entity>

